PROGRAM fibonacci
BEGIN

	STRING input := "Please input an integer number: ";
	STRING space := " ";
	STRING eol := "\n";

	FUNCTION INT F (INT n)
	BEGIN
          INT n1;

          n1 := n;

		IF (n1 > 2)
			RETURN F(n1-1)+F(n1-2);
		ENDIF
		IF (n1 = 0) --This is a comment
			RETURN 0;
		ELSE
			RETURN 1;
		ENDIF

	END


	FUNCTION VOID main ()
	BEGIN
		INT wharg;
		INT i, end, result;
		WRITE(input);
		READ(end);

	i := 0;
	WHILE (i != end)
		INT snarg;
		result := F(i);
		WRITE (i,space);
		IF (n1 = 0) --This is a comment
			FLOAT glarg;
		ELSE
			INT chug;
		ENDIF
		WRITE (result,eol);
		i := i + 1;
	ENDWHILE

	END

END
